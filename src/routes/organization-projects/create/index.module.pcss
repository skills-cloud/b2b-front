.project-create {

    &__form-wrapper {
        width: 592px;
        display: grid;
        grid-gap: 24px;
        margin-top: 32px;
    }

    &__separator {
        display: block;
        margin: 8px -32px 0;
        height: 1px;
        background-color: var(--color-separator-default);
    }

    &__submit-wrapper {
        display: flex;
        justify-content: flex-end;
    }
}
